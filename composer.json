		{
			"type": "package",
			"package": {
				"name": "jazzsequence/reviews",
				"version": "2.0.0",
				"type": "wordpress-plugin",
				"source": {
					"type": "git",
					"url": "https://github.com/jazzsequence/reviews",
					"reference": "tags/2.0.0"
				}
