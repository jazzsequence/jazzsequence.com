		"jazzsequence/cat-signal": "^1.1.3",
		"jazzsequence/games-collector": "1.3.4",
		"jazzsequence/hello-ziggy": "^2.1",
		"jazzsequence/notifications": "^1.1.3",
		"jazzsequence/progress-bar": "^2.1.4",
		"jazzsequence/recipe-box": "^0.3.2",
		"jazzsequence/releases": "^1.2",
		"jazzsequence/reviews": "^2.0",
		"jazzsequence/wp-show-tracker": "^0.6",
		"wpackagist-plugin/amazon-s3-and-cloudfront": "^2.4.4",
		"wpackagist-plugin/akismet": "^4.1.6",
        "wpackagist-plugin/cmb2": "^v2.7.0",
		"wpackagist-plugin/fix-rss-feed": "^3.1",
		"wpackagist-plugin/glotpress": "^2.3.1",
		"wpackagist-plugin/google-analyticator": "^6.5.5",
		"wpackagist-plugin/gutenberg": "^9.1.0",
		"wpackagist-plugin/jetpack": "^8.9.1",
		"wpackagist-plugin/ninja-forms": "^3.4.30",
		"wpackagist-plugin/organize-series": "^2.5.13",
		"wpackagist-plugin/p3-profiler": "^1.5.3",
		"wpackagist-plugin/plugins-garbage-collector": "^0.12",
		"wpackagist-plugin/quick-pagepost-redirect-plugin": "^5.2.0",
		"wpackagist-plugin/regenerate-thumbnails": "^3.1.3",
		"wpackagist-plugin/ssl-insecure-content-fixer": "^2.7.2",
		"wpackagist-plugin/storyftw": "^0.1.4",
		"wpackagist-plugin/wordpress-https": "^3.4.2",
		"wpackagist-plugin/wordpress-mu-domain-mapping": "^0.5.5",
		"wpackagist-plugin/wp-syntax": "^1.1",
		"wpackagist-plugin/yourls-link-creator": "^2.1.1",
		"wpackagist-plugin/yourls-wordpress-to-twitter": "^1.6.1",
        "wpackagist-theme/twentyseventeen": "^2.4",
        "wpackagist-theme/twentytwenty": "^1.5",
		"wpackagist-theme/museum-core": "^2.1.6"
    },
    "require-dev": {
        "altis/local-server": "^5.0.0-beta1"
    },
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Human Made",
            "email": "hello@humanmade.com"
        },
        {
            "name": "Chris Reynolds",
            "email": "me@chrisreynolds.io"
        }
    ],
    "extra": {
        "altis": {
            "modules": {
                "cloud": {
                    "xray": false,
                    "ludicrousdb": false,
                    "s3-uploads": false,
                    "email-from-address": "hello@chrisreynolds.io"
                },
                "cms": {
                    "default-theme": "twentyseventeen",
                    "remove-emoji": false
                },
                "media": {
                    "tachyon": false
                },
                "multilingual": false,
                "search": false,
                "security": {
                    "require-login": false,
                    "audit-log": false
                }
            }
        },
        "installer-paths": {
            "content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/cmb2"
            ],
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.2",
            "ext-mbstring": "7.2.30"
        }
    }
}
