{
    "name": "jazzsequence/jazzsequence.com",
    "description": "jazzsequence.com network",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "altis/altis": "^4.0.0",
        "wpackagist-plugin/cmb2": "^v2.7.0"
    },
    "require-dev": {
        "altis/local-server": "^4.0.0"
    },
    "authors": [
        {
            "name": "Human Made",
            "email": "hello@humanmade.com"
        }
    ],
    "extra": {
        "altis": {},
        "installer-paths": {
            "content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/cmb2"
            ],
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.2",
            "ext-mbstring": "7.2.30"
        }
    }
}
