          "package": {
            "name": "jazzsequence/artists",
            "version": "1.0.1",
            "type": "wordpress-plugin",
            "source": {
              "type": "git",
              "url": "https://github.com/jazzsequence/Artists",
              "reference": "tags/1.0.1"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "jazzsequence/cat-signal",
            "version": "1.1.3",
            "type": "wordpress-plugin",
            "source": {
              "type": "git",
              "url": "https://github.com/jazzsequence/cat-signal",
              "reference": "tags/1.1.3"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "jazzsequence/games-collector",
            "version": "1.3.4",
