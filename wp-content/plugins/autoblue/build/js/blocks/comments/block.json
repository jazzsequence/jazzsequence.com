{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "autoblue/comments",
  "title": "Autoblue Comments",
  "description": "Display Bluesky comments on your site.",
  "category": "embed",
  "attributes": {
    "url": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./index.php"
}